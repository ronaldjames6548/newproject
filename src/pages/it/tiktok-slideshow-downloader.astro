---
import Container from "@components/container.astro";
import Hero from "@components/hero.astro";
import Layout from "@layouts/Layout.astro";
import Features from "@components/features.astro";
import Newsletter from "@components/newsletter.astro";
import Testimonials from "@components/testimonials.astro";
import Faq from "@components/faq.astro";

import { changeLanguage } from "i18next";
import { t, changeLanguage } from "i18next";
changeLanguage("en");

const title = `Tiktokio: Downloader di presentazioni TikTok`;
const description = `Scarica la presentazione di TikTok con TikTokio Tiktok Downloader, tutte le foto della presentazione di TikTok senza filigrana online`;


---
<Layout title={title} description={description}>
<Container>
		<Hero heading="Tiktokio: downloader di presentazioni TikTok" subheading="Scarica la presentazione di foto e le immagini di TikTok senza filigrana"/>
    <div class="content">
      <div class="relative p-4">
        <div class="max-w-screen-xl mx-auto">
          <div class="mt-3 rounded-b lg:rounded-b-none lg:rounded-r flex flex-col justify-between leading-normal">
                <Features />
                <Newsletter />
                <Testimonials />
                <Faq />

          </div>
       </div>
      </div>
</div>
</Container>
</Layout>
